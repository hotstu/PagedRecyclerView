<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="layout_role" format="enum">
        <enum name="header" value="0" />
        <enum name="footer" value="1" />
        <enum name="content" value="2" />
    </attr>
    <declare-styleable name="BouncingFrameLayout">
